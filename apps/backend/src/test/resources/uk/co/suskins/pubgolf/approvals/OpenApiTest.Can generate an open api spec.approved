            "format" : "int32"
          },
          "randomise" : {
            "$ref" : "#/components/schemas/RandomiseOutcomeResponse"
          },
          "penalties" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/PenaltyResponse"
            }
          }
        },
        "required" : [ "id", "name", "penalties", "scores", "totalScore" ]
      },
      "RandomiseOutcomeResponse" : {
        "type" : "object",
        "properties" : {
          "hole" : {
            "type" : "integer",
            "format" : "int32"
          },
          "result" : {
            "type" : "string"
