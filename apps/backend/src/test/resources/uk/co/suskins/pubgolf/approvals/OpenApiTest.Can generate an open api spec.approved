            "type" : "string",
            "minLength" : 1
          }
        },
        "required" : [ "host" ]
      },
      "CreateGameResponse" : {
        "type" : "object",
        "properties" : {
          "gameId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "gameCode" : {
            "type" : "string"
          },
          "playerId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "playerName" : {
            "type" : "string"
          }
        },
        "required" : [ "gameCode", "gameId", "playerId", "playerName" ]
      },
      "ErrorResponse" : {
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        },
        "required" : [ "message" ]
      },
      "ScoreRequest" : {
        "type" : "object",
        "properties" : {
          "hole" : {
            "type" : "integer",
            "format" : "int32",
            "maximum" : 9,
            "minimum" : 1
          },
          "score" : {
            "type" : "integer",
            "format" : "int32",
            "maximum" : 10,
            "minimum" : -10
          },
          "penaltyType" : {
            "type" : "string",
            "enum" : [ "SKIP", "CHUNDER" ]
          }
        },
        "required" : [ "hole", "score" ]
      },
      "RandomiseResponse" : {
        "type" : "object",
        "properties" : {
          "result" : {
            "type" : "string"
          },
          "hole" : {
            "type" : "integer",
            "format" : "int32"
          }
        },
        "required" : [ "hole", "result" ]
      },
      "GameJoinRequest" : {
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "minLength" : 1
          }
        },
        "required" : [ "name" ]
      },
      "JoinGameResponse" : {
        "type" : "object",
        "properties" : {
          "gameId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "gameCode" : {
            "type" : "string"
          },
          "playerId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "playerName" : {
            "type" : "string"
          }
        },
        "required" : [ "gameCode", "gameId", "playerId", "playerName" ]
      },
      "ActivateEventRequest" : {
        "type" : "object",
        "properties" : {
          "playerId" : {
            "type" : "string",
            "format" : "uuid"
          }
        },
        "required" : [ "playerId" ]
      },
      "ActiveEventResponse" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string"
          },
          "title" : {
            "type" : "string"
          },
          "description" : {
            "type" : "string"
          },
          "activatedAt" : {
            "type" : "string",
            "format" : "date-time"
          }
        },
        "required" : [ "activatedAt", "description", "id", "title" ]
      },
      "GameStateResponse" : {
        "type" : "object",
        "properties" : {
          "gameId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "gameCode" : {
            "type" : "string"
          },
          "status" : {
            "type" : "string",
            "enum" : [ "ACTIVE", "COMPLETED" ]
          },
          "hostPlayerId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "players" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/PlayerResponse"
            }
          },
          "activeEvent" : {
            "$ref" : "#/components/schemas/ActiveEventResponse"
          }
        },
        "required" : [ "gameCode", "gameId", "players", "status" ]
      },
      "PenaltyResponse" : {
        "type" : "object",
        "properties" : {
          "hole" : {
            "type" : "integer",
            "format" : "int32"
          },
          "type" : {
            "type" : "string"
          },
          "points" : {
            "type" : "integer",
            "format" : "int32"
          }
        },
        "required" : [ "hole", "points", "type" ]
      },
      "PlayerResponse" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "format" : "uuid"
          },
          "name" : {
            "type" : "string"
          },
          "scores" : {
            "type" : "array",
            "items" : {
